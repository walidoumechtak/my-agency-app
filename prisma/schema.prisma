generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Agency {
  id          String   @id
  name        String?
  state       String?
  state_code  String?
  type        String?
  population  Int?
  website     String?
  county      String?
  created_at  DateTime? @map("created_at")
  updated_at  DateTime? @map("updated_at")

  contacts    Contact[]
}

model Contact {
  id          String   @id
  first_name  String?
  last_name   String?
  email       String?
  phone       String?
  title       String?
  email_type  String?
  department  String?
  created_at  DateTime? @map("created_at")
  updated_at  DateTime? @map("updated_at")

  agency_id   String?
  agency      Agency?  @relation(fields: [agency_id], references: [id])
}

model UserViewLimit {
  id      String   @id @default(cuid())
  userId  String
  date    DateTime
  count   Int
}
